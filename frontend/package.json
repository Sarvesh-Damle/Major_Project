    "axios": "^1.6.6",
    "esri-leaflet-geocoder": "2.3.3",
    "firebase": "^10.11.0",
    "framer-motion": "^11.0.18",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-accessible-accordion": "^5.0.0",
    "react-countup": "^6.5.0",
