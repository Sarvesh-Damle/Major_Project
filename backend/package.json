    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.0.1",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.1.0",
    "mongoose-aggregate-paginate-v2": "^1.0.7",
    "multer": "1.4.5-lts.1",
    "winston": "^3.19.0"
