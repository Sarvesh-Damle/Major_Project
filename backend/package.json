{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend API for Buddies property rental platform",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon -r dotenv/config src/index.js",
    "start": "node -r dotenv/config src/index.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "validate-env": "node -r dotenv/config scripts/validate-env.js"
